    <script>
         $(document).ready(function(){
            $('#txtbtn1').fadeOut(0);
            $('#txtbtn2').fadeOut(0);
            $('#txtbtn3').fadeOut(0);
            $('#btn1').click(function(){

                if(txtbtn1==false){
                    $(document).ready(function(){
                    $('#txtbtn1').fadeOut(100);
                    $('#btn1').animate({"backgroundColor":"white"},20);
                });
                }
                else {
                    $('#txtbtn1').fadeIn(300);
                    $('#btn1').css({"background-color":"plum"},20);
                   
               

                }
                txtbtn1=!txtbtn1;
            });
            $('#btn2').click(function () {

                 if (txtbtn2 == false) {
                     $(document).ready(function () {
                         $('#txtbtn2').fadeOut(100);
                         $('#btn2').animate({"backgroundColor":"white"},20);

                     });
                 }
                 else {
                     $('#txtbtn2').fadeIn(300);
                     $('#btn2').animate({ "backgroundColor": "#FF715B" }, 20);
                     $('#btn2').css({"background-color":"#FF715B"},20);


                 }
                 txtbtn2 = !txtbtn2;
            });
            $('#btn3').click(function () {

                 if (txtbtn3 == false) {
                     $(document).ready(function () {
                         $('#txtbtn3').fadeOut(100);
                         $('#btn3').animate({"backgroundColor":"white"},20);

                     });
                 }
                 else {
                     $('#txtbtn3').fadeIn(300);
                     $('#btn3').animate({ "backgroundColor": "#AFD5AA" }, 20);


                 }
                 txtbtn3 = !txtbtn3;
             });
            
            $('#btn1').mouseover(function(){
                if(txtbtn1==true){$(this).animate({backgroundColor:"plum"},200);}
                
            });
            $('#btn1').mouseleave(function(){
                 if(txtbtn1==true){$(this).animate({backgroundColor:"white"},100);}
                
            });
            $('#btn2').mouseover(function(){
                if(txtbtn2==true){$(this).animate({backgroundColor:"#FF715B"},200);}
                
            });
            $('#btn2').mouseleave(function(){
                if(txtbtn2==true){$(this).animate({backgroundColor:"white"},100);}
                
            });
            $('#btn3').mouseover(function(){
                if(txtbtn3==true){$(this).animate({backgroundColor:"#AFD5AA"},200);}
                
            });
            $('#btn3').mouseleave(function(){
                if(txtbtn3==true){$(this).animate({backgroundColor:"white"},100);}
                
            });

  
        });

        const value = [];
        const valueBtc = [];
        let color = "black";
        let colorBtc = "black";

        let txtbtn1=true;
        let txtbtn2=true;
        let txtbtn3=true;
        function check(arg, arg2)
        {
            var wallet = document.getElementById("wallet");
            var price = document.getElementById("price");
            var priceBtc = document.getElementById("price-btc");

            price.innerHTML=arg;
            priceBtc.innerHTML=arg2;

            if(value[0]>arg){ color = "#b02727"; }
            else if(value[0]==arg || value[0]==null){ color = "black";}
            else { color = "#29b027";  }

            if(valueBtc[0]>arg){ colorBtc = "#b02727"; }
            else if(valueBtc[0]==arg || valueBtc[0]==null){ colorBtc = "black";}
            else { colorBtc = "#29b027";  }


            $(document).ready(function(){
            $("#price").animate({color:"black"},5000);
            $("#price-btc").animate({color:"black"},5000);
            });
            
            price.style.color = color;
            priceBtc.style.color = colorBtc;
            value[0]=arg;
            valueBtc[0]=arg2;
            

        }
        function main()
        {
            fetch("/scrape.json")
            .then(response => response.json()) 
            .then(js => check(js.solana, js.bitcoin) );
            
            console.log("This script works!");
        }

     
    setInterval(main, 12000);
